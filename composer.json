{
  "name": "h4kuna/critical-cache",
  "type": "library",
  "description": "Only one process can to write or to delete to cache.",
  "license": "MIT",
  "authors": [
    {
      "name": "Milan Matějček",
      "email": "milan.matejcek@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.0",
    "h4kuna/memoize": "^0.1.3",
    "psr/simple-cache": "^3.0"
  },
  "require-dev": {
    "league/flysystem": "^3.12",
    "malkusch/lock": "^2.2",
    "nette/caching": "^3.2",
    "nette/tester": "^2.4",
    "nette/utils": "^3.2",
    "phpstan/phpstan": "^1.8",
    "phpstan/phpstan-strict-rules": "^1.4",
    "tracy/tracy": "^2.9"
  },
  "autoload": {
    "psr-4": {
      "h4kuna\\CriticalCache\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "h4kuna\\CriticalCache\\Tests\\": [
        "tests/src"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "phpstan": "vendor/bin/phpstan analyse",
    "tests": "vendor/bin/tester -s -j 4 --colors 1 -s -C tests/src"
  },
  "suggest": {
    "malkusch/lock": "As default implementation for Lock system.",
    "league/flysystem": "FlockMutex is used for create file stream.",
    "nette/utils": "FlockMutex need create sub directories.",
    "nette/caching": "As default implementation for Cache for PSR-16."
  }
}
